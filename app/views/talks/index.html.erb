<%= render "shared/header" %>
<%# みんなの投稿一覧　タイトルのみ%>
<div class='main'>
  <div class='talks-contents'>
    <h2 class='main-title'>投稿一覧</h2>
    <ul class='talks-lists'>
      <% @talks.each do |talk| %>
        <li class='list'>
          <%= link_to talk_path(talk) do%>
          <div class='talks-info'>
            <h3 class='talks-title'>
              <%= talk.title %>
            <h3>
          </div>
          <% end %>
        </li>
      <% end %>
    </ul>
    <%# 投稿一覧 %>
  </div>
</div>
<%= render "shared/footer" %>
  